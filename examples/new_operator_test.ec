// Test the new operator
console.log("Testing new operator...");

// Test Object constructor
var obj = new Object();
console.log("new Object():", obj);

// Test Array constructor  
var arr = new Array();
console.log("new Array():", arr);

var arr2 = new Array(3);
console.log("new Array(3):", arr2);

// Test String constructor
var str = new String("hello");
console.log("new String('hello'):", str);

// Test Number constructor
var num = new Number(42);
console.log("new Number(42):", num);

// Test Boolean constructor
var bool = new Boolean(true);
console.log("new Boolean(true):", bool);

console.log("New operator tests complete!");
