// Single-line Comments
// This is a single-line comment

var x = 42; // Comment at end of line
console.log("x:", x);

// You can have multiple single-line comments
// Each line needs its own // prefix
// Like this

var name = "ECEngine"; // Another end-of-line comment
console.log("Engine:", name);

// Comments are ignored by the interpreter
// var hidden = "This won't execute";

var visible = "This will execute";
console.log("Visible:", visible);
